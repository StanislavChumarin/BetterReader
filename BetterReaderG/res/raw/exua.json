{
    "filter1": {
        "url": "http://www.ex.ua/",
        "should_exist": ".include_0>tbody>tr>td>a>img",
        "layout_name": "single_list",
        "data": [
            {
                "type": "list",
                "id": "list_content",
                "selector": ".include_0>tbody>tr>td",
                "data": [
                    {
                        "type": "list_row",
                        "layout_name": "image_w_two_lines_list_row",
                        "data": [
                            {
                                "type":"image",
                                "id":"row_image",
                                "selector":"a>img"
                            },
                            {
                                "type": "text",
                                "id": "row_main_text",
                                "selector": "a>b"
                            },
                            {
                                "type": "text",
                                "id": "row_small_text",
                                "selector": ".info"
                            }
                        ]
                    }
                ],
                "action": {
                    "item": ".include_0>tbody>tr>td>p>a:eq(0):not(.info)",
                    "attr":"href"
                }
            }
        ]
    },
    "filter2": {
        "url": "http://www.ex.ua/",
        "should_exist": ".include_0",
        "layout_name": "single_list",
        "data": [
            {
                "type": "list",
                "id": "list_content",
                "selector": ".include_0>tbody>tr>td",
                "data": [
                    {
                        "type": "list_row",
                        "layout_name": "two_lines_list_row",
                        "data": [
                            {
                                "type": "text",
                                "id": "row_main_text",
                                "selector": "a>b"
                            },
                            {
                                "type": "text",
                                "id": "row_small_text",
                                "selector": ".info"
                            }
                        ]
                    }
                ],
                "action": {
                    "item": ".include_0>tbody>tr>td>a",
                    "attr":"href"
                }
            }
        ]
    },
    "filter3": {
        "url": "http://www.ex.ua/",
        "should_exist": ".menu_text",
        "layout_name": "single_list",
        "data": [
            {
                "type": "list",
                "id": "list_content",
                "selector": ".menu_text:eq(0)",
                "data": [
                    {
                        "type": "list_row",
                        "layout_name": "one_line_list_row",
                        "data": [
                            {
                                "type": "text",
                                "id": "text",
                                "selector": "a[href!=/]"
                            }
                        ]
                    }
                ],
                "action": {
                    "item": ".menu_text:eq(0)>a[href!=/]",
                    "attr":"href"
                }
            }
        ]
    }
}