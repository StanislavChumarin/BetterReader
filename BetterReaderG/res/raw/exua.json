{
   "filter1" :
      {
         "url" : "http://www.ex.ua/",
         "should_exist" : ".list",
         "screens" :
            [
               {
                  "layout_name" : "description_with_image_and_file_list",
                  "title_selector" : "title",
                  "data" :
                     [
                        {
                           "type" : "text",
                           "id" : "title_text",
                           "selector" : "td>h1",
                           "attr" : "text"
                        },
                        {
                           "type" : "image",
                           "id" : "big_image",
                           "selector" : "#body_element tr:eq(0) img",
                           "attr" : "src"
                        },
                        {
                           "type" : "text",
                           "id" : "description_text",
                           "selector" : "#body_element td:eq(0) p:not(:has(.r_button))",
                           "attr" : "text"
                        },
                        {
                           "type" : "list",
                           "id" : "list_content",
                           "selector" : ".list tr:not(:eq(0))",
                           "data" :
                              [
                                 {
                                    "type" : "list_row",
                                    "layout_name" : "image_w_two_lines_list_row",
                                    "data" :
                                       [
                                          {
                                             "type" : "image",
                                             "id" : "row_image",
                                             "selector" : "a>img",
                                             "attr" : "src"
                                          },
                                          {
                                             "type" : "text",
                                             "id" : "row_main_text",
                                             "selector" : "td:eq(1)>a",
                                             "attr" : "text"
                                          },
                                          {
                                             "type" : "text",
                                             "id" : "row_small_text",
                                             "selector" : ".small b",
                                             "attr" : "text"
                                          }
                                       ]
                                 }
                              ]
                        }
                     ]
               }
            ]
      },
   "filter2" :
      {
         "url" : "http://www.ex.ua/",
         "should_exist" : ".include_0>tbody>tr>td>a>img",
         "screens" :
            [
               {
                  "layout_name" : "single_list",
                  "title_selector" : "title",
                  "data" :
                     [
                        {
                           "type" : "list",
                           "id" : "list_content",
                           "selector" : ".include_0>tbody>tr>td",
                           "data" :
                              [
                                 {
                                    "type" : "list_row",
                                    "layout_name" : "image_w_two_lines_list_row",
                                    "data" :
                                       [
                                          {
                                             "type" : "image",
                                             "id" : "row_image",
                                             "selector" : "a>img",
                                             "attr" : "src"
                                          },
                                          {
                                             "type" : "text",
                                             "id" : "row_main_text",
                                             "selector" : "a>b",
                                             "attr" : "text"
                                          },
                                          {
                                             "type" : "text",
                                             "id" : "row_small_text",
                                             "selector" : ".info",
                                             "attr" : "text"
                                          }
                                       ]
                                 }
                              ],
                           "click" :
                              {
                                 "action" : "open",
                                 "selector" : ".include_0>tbody>tr>td>p>a:eq(0):not(.info)",
                                 "attr" : "href"
                              },
                           "pager" :
                              {
                                 "selector" : ".include_0 ~ table td:has(img[src*=arr_r]) a",
                                 "attr" : "href"
                              }
                        }
                     ]
               }
            ]
      },
